{
  "name": "cucumberjs-zephyr-formatter",
  "version": "0.0.0-development",
  "description": "Interacts with ZAPI and JIRA API to report on Zephyr test status",
  "main": "build/main/index.js",
  "files": [
    "build"
  ],
  "typings": "build/main/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chattersley/cucumberjs-zephyr-formatter.git"
  },
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "dev": "run-s build && run-s cucumber",
    "cucumber": "cucumber-js __component_tests__/features/**/*.feature --logLevel=error --require __component_tests__/steps/**/*.ts --require __component_tests__/hooks/**/*.ts --require-module ts-node/register --format build/main/formatter/JiraTestReporter.js",
    "info": "npm-scripts-info",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.imports.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier src/**/*.ts --write --config .prettierrc",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s test:*",
    "test:lint": "tslint --exclude 'src/**/*.d.ts, src/**/*.json' --project . && prettier src/**/*.ts --list-different --config .prettierrc",
    "test:unit": "jest --config jest.config.js",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "cov": "jest --coverage",
    "doc": "run-s doc:html && opn build/docs/index.html",
    "doc:html": "typedoc src/ --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash build test",
    "all": "run-s reset test cov:check doc:html",
    "prepare-release": "run-s all version doc:publish",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "release": "run-s build version",
    "version": "semantic-release --allow-same-version"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "dependencies": {
    "@types/cucumber": "^4.0.4",
    "@types/dotenv": "^4.0.3",
    "@types/jira-client": "^6.4.0",
    "@types/lodash": "^4.14.118",
    "assertion-error-formatter": "^2.0.1",
    "axios": "^0.18.0",
    "class-validator": "^0.9.1",
    "dotenv": "^6.2.0",
    "get-project-version": "^1.0.3",
    "http-status-codes": "^1.3.0",
    "install": "^0.12.1",
    "jira-client": "^6.4.1",
    "lodash": "^4.17.11",
    "project-name": "^1.0.0",
    "sha.js": "^2.4.11",
    "typescript-collections": "^1.3.2",
    "typescript-logging": "^0.6.3"
  },
  "devDependencies": {
    "@types/expect": "^1.20.4",
    "@types/jest": "^24.0.5",
    "codecov": "^3.1.0",
    "cucumber": "^5.1.0",
    "event-stream": "^4.0.1",
    "expect": "^24.1.0",
    "fs": "0.0.1-security",
    "gh-pages": "^2.0.0",
    "jest": "^23.6.0",
    "jest-mock-axios": "^2.1.11",
    "npm-run-all": "^4.1.3",
    "opn-cli": "^3.1.0",
    "prettier": "^1.14.3",
    "prettier-tslint": "^0.4.0",
    "prettierrc": "0.0.0",
    "semantic-release": "^15.3.2",
    "semver": "^5.6.0",
    "trash-cli": "^1.4.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.3.3"
  }
}
